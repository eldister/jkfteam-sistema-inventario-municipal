<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>AccionesActivos</servlet-name>
        <servlet-class>simuni.servlets.AccionesArticulos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccionesProveedores</servlet-name>
        <servlet-class>simuni.servlets.AccionesProveedoresF</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccionesUsuarios</servlet-name>
        <servlet-class>simuni.servlets.AccionesUsuarios</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccionesNotificaciones</servlet-name>
        <servlet-class>simuni.servlets.AccionesNotificaciones</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AccionesActivos</servlet-name>
        <url-pattern>/modulos/activos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccionesProveedores</servlet-name>
        <url-pattern>/modulos/proveedores</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccionesUsuarios</servlet-name>
        <url-pattern>/AccionesUsuarios</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccionesNotificaciones</servlet-name>
        <url-pattern>/AccionesNotificaciones</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
